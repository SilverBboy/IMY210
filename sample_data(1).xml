<?xml version="1.0"?>
<faqs>
	<!-- <faqs> can hold infinite <module> itemss-->
	<module code="IMY210" name="Advance Markup Languages">
		<!-- 	
			<module> can contain infinite <faq>
				
			@code should be a 6 character identifier and must appear 
			@code need to follow the structure of 3 character followed by 3 numeric value	

			@name can contain a string no longer than 50 characters
		-->
		<faq priority="23">
		<!-- 	
			<faq> should follow the structure of <tags>, <question>, <answer> 	

			@priority holds a positive numeric value 
			@priority shows which FAQ should be displayed first if multiple exist
		-->	 
			<tags>
			<!-- 	
				<tags> can hold up to five <tags>
			-->	
				<tag>String no longer than 25 characters</tag>
				<tag>String no longer than 25 characters</tag>
			</tags>
			<question>String no longer than 255 characters</question>
			<answer>String no longer than 255 characters</answer>
		</faq>

		<!-- sample -->
		<faq priority="20">
			<tags>
				<tag>Study guide</tag>
			</tags>
			<question>Where can I get the study guide?</question>
			<answer>ClickUP</answer>
		</faq>
	</module>
</faqs>